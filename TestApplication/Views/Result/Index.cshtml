@model IEnumerable<Model.Models.Result>


<div>
    @if (Model.Count() > 0)
    {
        <table class="table table-striped border">
            <tr class="table-secondary">

                <th> Hotel Id</th>
                <th>Test Id</th>
                <th>Result</th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>

                    <td>
                        @item.HotelId
                    </td>
                    <td>
                        @item.TestId
                    </td>

                    <td>
                        @item.Output
                    </td>
                </tr>
            }
        </table>
    }
    else
    {
        <p>Listelenecek herhangi bir otel bulunamadı</p>
    }
</div>